{
  "name": "kodelens",
  "displayName": "kodelens-vscode",
  "publisher": "kodelens",
  "description": "Talk to your code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:kodelens.askQuestion",
    "onCommand:kodelens.findReferences",
    "onCommand:kodelens.parseWorkspace",
    "onCommand:kodelens.parseApex"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kodelens.parseApex",
        "title": "Kodelens: Parse Apex File"
      },
      {
        "command": "kodelens.askQuestion",
        "title": "Kodelens: Ask a Question"
      },
      {
        "command": "kodelens.parseWorkspace",
        "title": "Kodelens: Parse Workspace"
      },
      {
        "command": "kodelens.findReferences",
        "title": "KodeLens: Find All References"
      }
    ],
    "configuration": {
      "title": "Kodelens",
      "properties": {
        "kodelens.embeddingModel": {
          "type": "string",
          "default": "rust",
          "enum": [
            "mock",
            "local",
            "rust"
          ],
          "description": "Choose embedding model: 'rust', 'mock', 'local'"
        },
        "kodelens.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for cloud embedding services"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "kodelens-results",
          "name": "KodeLens Results"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "kodelens-results",
        "contents": "No search results yet. Use [KodeLens: Find All References](command:kodelens.findReferences) to find references to a symbol."
      }
    ]
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "compile": "npm run check-types && npm run lint && node esbuild.js --production",
    "package": "npm run compile && npx vsce package",
    "reinstall": "npm run compile && npm run package && code --uninstall-extension kodelens.kodelens && code --install-extension kodelens-0.0.1.vsix",
    "build-apex-wasm": "cd tree-sitter-sfapex/apex && tree-sitter build --wasm && mkdir -p ../../media/apex && cp tree-sitter-apex.wasm ../../media/apex/",
    "vscode:prepublish": "npm run compile",
    "test": "jest",
    "clean": "rm -rf dist",
    "postinstall": "npx electron-rebuild -f -w better-sqlite3 --version 37.3.1"
  },
  "devDependencies": {
    "@esbuild/darwin-x64": "^0.25.10",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.4",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/vsce": "^3.6.0",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.25.10",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.32.0",
    "jest": "^30.1.3",
    "tree-sitter-sfapex": "github:aheber/tree-sitter-sfapex#3597575",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jindalsaurabh/kodelens.git"
  },
  "dependencies": {
    "@huggingface/inference": "^4.8.0",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^12.2.0",
    "bindings": "^1.5.0",
    "dotenv": "^17.2.2",
    "file-uri-to-path": "^2.0.0",
    "fs-extra": "^11.3.2",
    "google-auth-library": "^10.3.0",
    "module-alias": "^2.2.3",
    "openai": "^5.20.2",
    "web-tree-sitter": "^0.20.8"
  },
  "_moduleAliases": {
    "vscode": "scripts/mocks/vscode"
  },
  "vsce": {
    "includeDependencies": false
  }
}
